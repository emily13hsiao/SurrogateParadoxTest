\name{a_b_hat}
\alias{a_b_hat}

\title{
Helper function
}
\description{
Helper function for monotonicity test
}
\usage{
a_b_hat(r, s, X, Y)
}

\arguments{
  \item{r}{
Index to start summation.
}
  \item{s}{
Index to end summation.
}
  \item{X}{
Vector of X values.
}
  \item{Y}{
Vector of Y values.
}
}

\value{
\item{a_hat}{Numeric value of a_hat}
\item{b_hat}{Numeric value of b_hat}
}

\author{
Emily Hsiao
}

\keyword{arith}
\keyword{internal}
